{
  "name": "commit-analyzer-scoped",
  "description": "semantic-release plugin to analyze commits with conventional-changelog",
  "version": "0.0.0",
  "author": "La Javaness <dev@lajavaness.com>",
  "ava": {
    "files": [
      "test/**/*.test.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/La-Javaness/commit-analyzer-scoped/issues"
  },
  "contributors": [
    "Stephan BÃ¶nnemann <stephan@boennemann.me> (http://boennemann.me)",
    "Gregor Martynus (https://twitter.com/gr2m)",
    "Pierre Vanduynslager (https://twitter.com/@pvdlg_)",
    "Kay Yakimenko <commit-analyzer-scoped@juke.fr>",
    "Vincent Le Badezet <vincent@lajavaness.com>"
  ],
  "dependencies": {
    "conventional-changelog-angular": "^5.0.0",
    "conventional-commits-filter": "^2.0.0",
    "conventional-commits-parser": "^3.2.3",
    "debug": "^4.0.0",
    "import-from": "^4.0.0",
    "lodash": "^4.17.4",
    "micromatch": "^4.0.2"
  },
  "devDependencies": {
    "ava": "4.2.0",
    "conventional-changelog-atom": "2.0.8",
    "conventional-changelog-conventionalcommits": "4.6.3",
    "conventional-changelog-ember": "2.0.9",
    "conventional-changelog-eslint": "3.0.9",
    "conventional-changelog-express": "2.0.6",
    "conventional-changelog-jshint": "2.0.9",
    "nyc": "15.1.0",
    "semantic-release": "19.0.2",
    "sinon": "13.0.2",
    "xo": "0.32.1"
  },
  "engines": {
    "node": ">=14.17"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "homepage": "https://github.com/La-Javaness/commit-analyzer-scoped#readme",
  "keywords": [
    "changelog",
    "commit-analyzer",
    "conventional-changelog",
    "conventional-commits",
    "github",
    "publish",
    "release",
    "semantic-release"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "include": [
      "lib/**/*.js",
      "index.js"
    ],
    "reporter": [
      "json",
      "text",
      "html"
    ],
    "all": true
  },
  "peerDependencies": {
    "semantic-release": ">=18.0.0-beta.1"
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/La-Javaness/commit-analyzer-scoped.git"
  },
  "scripts": {
    "lint": "xo",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v",
    "test:ci": "nyc ava -v"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "rules": {
      "unicorn/string-content": "off"
    }
  },
  "renovate": {
    "extends": [
      "github>semantic-release/.github"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "tagFormat": "v${version}-scoped",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "chore",
              "release": "patch"
            }
          ]
        }
      ]
    ]
  }
}